form.ui.form(name="controlForm", ng-submit="kMeans()", ng-class="{'loading': computing}")
    div.inline.field
        label #{__('中心点个数 k')}:
        input(type="number", ng-model="kMeansData.k", placeholder="#{__('k 值 , 即类别个数')}")
    div.inline.field
        label #{__('最大迭代次数:')}
        input(type="number", ng-model="kMeansData.maxLoops", placeholder="#{__('最大迭代次数')}")
    div.field
        button.ui.button(type="submit") #{__('开始聚类')}

    div
        h5 #{__('数据:')}
        div#data-table